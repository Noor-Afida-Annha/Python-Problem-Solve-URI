a,b,c=map(int,input().split())
if a<b and a<c:
    print(a)
    if b<c:
        print(b)
        print(c)
    elif c<b:
        print(c)
        print(b)
elif b<a and b<c:
    print(b)
    if a < c:
        print(a)
        print(c)
    elif c < a:
        print(c)
        print(a)

else:
    print(c)
    if b<a:
        print(b)
        print(a)
    elif a<b:
        print(a)
        print(b)
print("")
print(a)
print(b)
print(c)
