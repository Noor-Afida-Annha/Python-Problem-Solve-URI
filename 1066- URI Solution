e=0
o=0
p=0
ne=0
for i in range(5):
    n=int(input())
    if n%2==0:
        e+=1
    if n%2!=0:
        o+=1
    if n>0:
        p+=1
    if n<0:
        ne+=1


print(f"{e} valor(es) par(es)")
print(f"{o} valor(es) impar(es)")
print(f"{p} valor(es) positivo(s)")
print(f"{ne} valor(es) negativo(s)")
